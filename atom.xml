---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Kamil Rudnicki</title>
 <link href="http://rudnicki.info/atom.xml" rel="self"/>
 <link href="http://rudnicki.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://rudnicki.info/</id>
 <author>
   <name>Kamil Rudnicki</name>
   <email>kamil.rudnicki@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://rudnicki.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://rudnicki.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>